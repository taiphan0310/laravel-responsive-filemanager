{
    "name": "taiphan0310/laravel-responsive-filemanager",
    "description": "Make responsive filemanager for laravel",
    "autoload": {
        "psr-4": {
            "Taiphan0310\\LaravelResponsiveFilemanager\\": "src/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "Taiphan0310\\ResponsiveFilemanagerServiceProvider\\"
            ]
        }
    },
    "authors": [
        {
            "name": "taiphan0310",
            "email": "taiphan0310@gmail.com"
        }
    ],
    "require": {
        "laravel/framework" : "^8.63",
        "php": ">=7.4.0",
        "ext-exif": "*",
        "ext-fileinfo": "*",
        "intervention/image": "2.*",
        "illuminate/config": "5.4.* || 5.5.* || 5.6.* || 5.7.* || 5.8.* || ^6.0 || ^7.0 || ^8.0",
        "illuminate/filesystem": "5.4.* || 5.5.* || 5.6.* || 5.7.* || 5.8.* || ^6.0 || ^7.0 || ^8.0",
        "illuminate/support": "5.4.* || 5.5.* || 5.6.* || 5.7.* || 5.8.* || ^6.0 || ^7.0 || ^8.0",
        "illuminate/http": "5.4.* || 5.5.* || 5.6.* || 5.7.* || 5.8.* || ^6.0 || ^7.0 || ^8.0",
        "illuminate/container": "5.4.* || 5.5.* || 5.6.* || 5.7.* || 5.8.* || ^6.0 || ^7.0 || ^8.0"
    }
}
